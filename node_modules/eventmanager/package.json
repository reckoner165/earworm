{
  "name": "eventmanager",
  "version": "2.0.1",
  "description": "Simple event library for JavaScript.",
  "repository": {
    "type": "git",
    "url": "git@github.vimeows.com:Player/eventmanager.git"
  },
  "author": "Brad Dougherty <brad@vimeo.com>",
  "main": "dist/eventmanager.js",
  "jsnext:main": "src/eventmanager.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean -s",
    "build": "rollup -c",
    "lint": "eslint src/ test/",
    "pretest": "npm run build",
    "test": "nyc --cache --reporter=text --reporter=html ava",
    "watch:build": "npm run build -- --watch",
    "watch:lint": "chokidar 'src/**/*.js' 'test/**/*.js' -c 'eslint {path}'",
    "watch:test": "ava --watch",
    "watch": "parallelshell 'npm run watch:build -s' 'npm run watch:lint -s' 'npm run watch:test -s'",
    "preversion": "npm run lint && npm test",
    "version": "npm run build && git add -A dist"
  },
  "private": true,
  "devDependencies": {
    "ava": "^0.15.2",
    "chokidar-cli": "^1.2.0",
    "eslint": "^2.1.0",
    "eslint-config-vimeo-player": "git+ssh://git@github.vimeows.com:Player/eslint-config.git#v3.0.0",
    "eslint-plugin-ava": "^2.4.0",
    "nyc": "^6.4.4",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.5.2",
    "rollup": "^0.29.0",
    "rollup-watch": "^2.1.1",
    "sinon": "^1.17.3"
  }
}
