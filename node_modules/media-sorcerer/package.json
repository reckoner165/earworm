{
  "name": "media-sorcerer",
  "version": "3.1.6",
  "description": "Mediasource api wrapper",
  "main": "dist/mediasorcerer.umd.js",
  "jsnext:main": "dist/mediasorcerer.es.js",
  "repository": {
    "type": "git",
    "url": "git@github.vimeows.com:player/media-sorcerer.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "node rollup.js",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty src/ test/",
    "pretest": "npm run lint",
    "test": "karma start --single-run",
    "watch:build": "npm run build -- --watch",
    "watch:lint": "chokidar 'src/**/*.js' 'test/**/*.js' -c 'eslint {path}'",
    "watch:test": "karma start",
    "watch": "parallelshell 'npm run watch:build -s' 'npm run watch:lint -s' 'npm run watch:test -s'",
    "preversion": "npm run lint && npm test",
    "version": "npm run build && git add -A dist",
    "docs": "jsdoc src/** --destination docs"
  },
  "keywords": [
    "video"
  ],
  "author": "Craig Campbell <craig@vimeo.com>",
  "license": "N/A",
  "devDependencies": {
    "@vimeo/eslint-config-player": "5.0.0",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-plugin-istanbul": "3.0.0",
    "babel-preset-es2015": "6.18.0",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "chokidar": "1.6.0",
    "electron-prebuilt": "1.3.4",
    "eslint": "3.14.1",
    "eslint-formatter-pretty": "1.1.0",
    "grunt": "1.0.1",
    "karma": "1.3.0",
    "karma-chai": "0.1.0",
    "karma-chai-as-promised": "0.1.2",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-electron-launcher": "0.1.0",
    "karma-mocha": "1.3.0",
    "karma-rollup-plugin": "0.2.4",
    "karma-sinon-chai": "1.2.4",
    "mocha": "3.2.0",
    "parallelshell": "2.0.0",
    "rimraf": "2.5.4",
    "rollup": "0.36.4",
    "rollup-plugin-babel": "2.6.1",
    "rollup-plugin-bundle-size": "1.0.1",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-sourcemaps": "0.4.1",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0"
  },
  "dependencies": {
    "eventmanager": "git+ssh://git@github.vimeows.com/player/eventmanager.git#v2.0.1",
    "jsdoc": "^3.4.3"
  }
}
